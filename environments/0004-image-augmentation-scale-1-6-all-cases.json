{
  "name": "0004 Image Augmentation Scale 1x to 6x",
  "description": "Augmentation 1x to 6x",
  "batch_size": 1,
  "epochs": 1000,
  "max_image_side_length": 512,
  "train_dataset_name": "cases",
  "dataset_split": [0.0, 0.0, 1.0],
  "shuffle_dataset": true,
  "shuffle_seed": 0,
  "learning_rate": 0.0001,
  "augmentation": {
    "type": "affine",
    "params": {
      "scale": "(1.0, 6.0)",
      "translate_percent": {
        "x": "(-0.5, 0.5)",
        "y": "(-0.5, 0.5)"
      }
    }
  },
  "simplify_classes": true
}