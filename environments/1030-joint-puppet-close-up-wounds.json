{
  "name": "1010 Image Augmentation",
  "description": "Default Network Configurations using Image Augmentation",
  "batch_size": 10,
  "max_epochs": 1000,
  "shuffle_dataset": true,
  "shuffle_seed": 0,
  "simplify_classes": true,
  "datasets": [
    {
      "name": "puppet_measure_bands",
      "split": [
        0.67,
        0.165,
        0.165
      ],
      "augmentation": {
        "type": "sequential",
        "params": {
          "children": [
            {
              "type": "fliplr",
              "params": {
                "p": 0.5
              }
            },
            {
              "type": "flipud",
              "params": {
                "p": 0.5
              }
            },
            {
              "type": "affine",
              "params": {
                "scale": {
                  "type": "Add",
                  "params": {
                    "other_param": {
                      "type": "Absolute",
                      "params": {
                        "other_param": {
                          "type": "Normal",
                          "params": {
                            "loc": 1.0,
                            "scale": 4.0
                          }
                        }
                      }
                    },
                    "val": 1.0
                  }
                },
                "translate_percent": {
                  "x": "(-0.5, 0.5)",
                  "y": "(-0.5, 0.5)"
                },
                "rotate": "[0.0, 90.0, 180.0, 270.0]"
              }
            },
            {
              "type": "sometimes",
              "params": {
                "p": 0.2
              },
              "children": [
                {
                  "type": "DefocusBlur",
                  "params": {
                    "severity": 1
                  }
                }
              ]
            },
            {
              "type": "LogContrast",
              "params": {
                "gain": "(0.7, 1.3)"
              }
            },
            {
              "type": "MultiplySaturation",
              "params": {
                "mul": "(0.7, 1.3)"
              }
            },
            {
              "type": "ChangeColorTemperature",
              "params": {
                "kelvin": "(4000, 9000)"
              }
            },
            {
              "type": "Cutout",
              "params": {
                "size": "(0.01, 0.05)",
                "nb_iterations": "(2, 20)",
                "fill_mode": "gaussian"
              }
            }
          ]
        }
      }
    },
    {
      "name": "closeup_wounds",
      "split": [
        0.67,
        0.165,
        0.165
      ],
      "augmentation": {
        "type": "sequential",
        "params": {
          "children": [
            {
              "type": "fliplr",
              "params": {
                "p": 0.5
              }
            },
            {
              "type": "flipud",
              "params": {
                "p": 0.5
              }
            },
            {
              "type": "affine",
              "params": {
                "scale": {
                  "type": "Add",
                  "params": {
                    "other_param": {
                      "type": "Absolute",
                      "params": {
                        "other_param": {
                          "type": "Normal",
                          "params": {
                            "loc": 1.0,
                            "scale": 4.0
                          }
                        }
                      }
                    },
                    "val": 1.0
                  }
                },
                "translate_percent": {
                  "x": "(-0.5, 0.5)",
                  "y": "(-0.5, 0.5)"
                },
                "rotate": "[0.0, 90.0, 180.0, 270.0]"
              }
            },
            {
              "type": "sometimes",
              "params": {
                "p": 0.2
              },
              "children": [
                {
                  "type": "DefocusBlur",
                  "params": {
                    "severity": 1
                  }
                }
              ]
            },
            {
              "type": "LogContrast",
              "params": {
                "gain": "(0.7, 1.3)"
              }
            },
            {
              "type": "MultiplySaturation",
              "params": {
