{
  "name": "1030 Joint Puppet Closeup Wounds",
  "description": "Default Network Configurations, Puppet and Close up wounds joint training",
  "batch_size": 5,
  "max_epochs": 1000,
  "shuffle_dataset": true,
  "shuffle_seed": 0,
  "simplify_classes": true,
  "use_transfer_learning": true,
  "allow_base_layer_training": false,
  "k_fold_x_val": 4,
  "x_val_k": "auto",
  "datasets": [
    {
      "name": "closeup_wounds",
      "split": [
        0.67,
        0.165,
        0.165
      ],
      "augmentation": {
        "type": "sequential",
        "params": {
          "children": [
            {
              "type": "fliplr",
              "params": {
                "p": 0.1
              }
            },
            {
              "type": "affine",
              "params": {
                "scale": {
                  "x": "(0.8, 1.2)",
                  "y": "(0.8, 1.2)"
                },
                "translate_percent": {
                  "x": "(-0.3, 0.3)",
                  "y": "(-0.3, 0.3)"
                },
                "rotate": "[0.0, 90.0, 270.0]"
              }
            },
            {
              "type": "sometimes",
              "params": {
                "p": 0.1
              },
              "children": [
                {
                  "type": "DefocusBlur",
                  "params": {
                    "severity": 1
                  }
                }
              ]
            },
            {
              "type": "LogContrast",
              "params": {
                "gain": "(0.9, 1.1)"
              }
            },
            {
              "type": "MultiplySaturation",
              "params": {
                "mul": "(0.9, 1.1)"
              }
            },
            {
              "type": "ChangeColorTemperature",
              "params": {
                "kelvin": "(6000, 7000)"
              }
            },
            {
              "type": "Cutout",
              "params": {
                "size": "(0.01, 0.05)",
                "nb_iterations": "(2, 10)",
                "fill_mode": "gaussian"
              }
            }
          ]
        }
      }
    },
    {
      "name": "puppet_measure_bands",
      "split": [
        0.67,
        0.165,
        0.165
      ],
      "augmentation": {
        "type": "sequential",
        "params": {
          "children": [
            {
              "type": "fliplr",
              "params": {
                "p": 0.1
              }
            },
            {
              "type": "affine",
              "params": {
                "scale": {
                  "type": "Add",
                  "params": {
                    "other_param": {
                      "type": "Absolute",
                      "params": {
                        "other_param": {
                          "type": "Normal",
                          "params": {
                            "loc": 1.0,
                            "scale": 2.0
                          }
                        }
                      }
                    },
                    "val": 1.0
                  }
                },
                "translate_percent": {
                  "x": "(-0.3, 0.3)",
                  "y": "(-0.3, 0.3)"
                },
                "rotate": "[0.0, 90.0, 270.0]"
              }
            },
            {
              "type": "sometimes",
              "params": {
                "p": 0.1
              },
              "children": [
                {
                  "type": "DefocusBlur",
                  "params": {
                    "severity": 1
                  }
                }
              ]
            },
            {
              "type": "LogContrast",
              "params": {
                "gain": "(0.9, 1.1)"
              }
            },
            {
              "type": "MultiplySaturation",
              "params": {
                "mul": "(0.9, 1.1)"
              }
            },
            {
              "type": "ChangeColorTemperature",
              "params": {
                "kelvin": "(6000, 7000)"
              }
            },
            {
              "type": "Cutout",
              "params": {
                "size": "(0.01, 0.05)",
                "nb_iterations": "(2, 10)",
                "fill_mode": "gaussian"
              }
            }
          ]
        }
      }
    }
  ]
}
