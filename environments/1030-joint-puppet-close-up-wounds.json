              }
            },
            {
              "type": "ChangeColorTemperature",
              "params": {
                "kelvin": "(4000, 9000)"
              }
            },
            {
              "type": "Cutout",
              "params": {
                "size": "(0.01, 0.05)",
                "nb_iterations": "(2, 20)",
                "fill_mode": "gaussian"
              }
            }
          ]
        }
      }
    }
  ]
}
