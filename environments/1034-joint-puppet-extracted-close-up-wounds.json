{
  "name": "1034 Joint Puppet Extracted Closeup Wounds",
  "description": "Default Network Configurations, Puppet and Close up wounds joint training",
  "batch_size": 20,
  "max_epochs": 1000,
  "shuffle_dataset": true,
  "shuffle_seed": 0,
  "simplify_classes": true,
  "use_transfer_learning": true,
  "allow_base_layer_training": false,
  "k_fold_x_val": 4,
  "x_val_k": "auto",
  "datasets": [
    {
      "name": "closeup_wounds",
      "split": [
        0.67,
        0.165,
        0.165
      ],
      "augmentation": {
        "type": "sequential",
        "params": {
          "children": [
            {
              "type": "fliplr",
              "params": {
                "p": 0.1
              }
            },
            {
              "type": "flipud",
              "params": {
                "p": 0.5
              }
            },
            {
              "type": "affine",
              "params": {
                "scale": {
                  "x": "(0.8, 1.2)",
                  "y": "(0.8, 1.2)"
                },
                "translate_percent": {
                  "x": "(-0.3, 0.3)",
                  "y": "(-0.3, 0.3)"
                },
                "rotate": "[0.0, 90.0, 180.0, 270.0]"
              }
            },
            {
              "type": "sometimes",
              "params": {
                "p": 0.2
              },
              "children": [
                {
                  "type": "DefocusBlur",
                  "params": {
                    "severity": 1
                  }
                }
              ]
            },
            {
              "type": "LogContrast",
              "params": {
                "gain": "(0.7, 1.3)"
              }
            },
            {
              "type": "MultiplySaturation",
              "params": {
                "mul": "(0.7, 1.3)"
              }
            },
            {
              "type": "ChangeColorTemperature",
              "params": {
                "kelvin": "(4000, 9000)"
              }
            },
            {
              "type": "Cutout",
              "params": {
                "size": "(0.01, 0.05)",
                "nb_iterations": "(2, 20)",
                "fill_mode": "gaussian"
              }
            }
          ]
        }
      }
    },
    {
      "name": "puppet_measure_bands_closeups",
      "split": [
        0.67,
        0.165,
        0.165
      ],
      "augmentation": {
        "type": "sequential",
        "params": {
          "children": [
            {
              "type": "fliplr",
              "params": {
                "p": 0.5
              }
            },
            {
              "type": "flipud",
              "params": {
                "p": 0.5
              }
            },
            {
              "type": "affine",
              "params": {
                "scale": {
                  "x": "(0.8, 1.2)",
                  "y": "(0.8, 1.2)"
                },
                "translate_percent": {
                  "x": "(-0.3, 0.3)",
                  "y": "(-0.3, 0.3)"
                },
                "rotate": "[0.0, 90.0, 180.0, 270.0]"
              }
            },
            {
              "type": "sometimes",
              "params": {
                "p": 0.2
              },
              "children": [
                {
                  "type": "DefocusBlur",
                  "params": {
                    "severity": 1
                  }
                }
              ]
            },
            {
              "type": "LogContrast",
              "params": {
                "gain": "(0.7, 1.3)"
              }
            },
            {
              "type": "MultiplySaturation",
              "params": {
                "mul": "(0.7, 1.3)"
              }
            },
            {
              "type": "ChangeColorTemperature",
              "params": {
                "kelvin": "(4000, 9000)"
              }
            },
            {
              "type": "Cutout",
              "params": {
                "size": "(0.01, 0.05)",
                "nb_iterations": "(2, 20)",
                "fill_mode": "gaussian"
              }
            }
          ]
        }
      }
    }
  ]
}
