  "use_transfer_learning": true,
  "allow_base_layer_training": true,
  "augmentation": {
    "type": "affine",
    "params": {
      "scale": "(1.0, 5.0)",
      "translate_percent": {
        "x": "(-0.5, 0.5)",
        "y": "(-0.5, 0.5)"
      }
    }
  },
  "center_color_to_imagenet": true,
  "simplify_classes": true
}